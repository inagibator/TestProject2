//********************************
//  Set Column Global Context
//********************************
var
  GRC : TK_RAFColumnDescr;
  GColumns:  ArrayOf TK_RAFColumnDescr;
  GIndex:  Integer;
  GWViewers : ArrayOf String;
  GWEditors : ArrayOf String;
  N_DisableField: Integer;

//************************************************************ N_InitCommon
// Init Common field
//
procedure N_InitCommon( UDSelf : TN_UDBase );
var
  Self : TK_FrameDescription;
begin
  Self = UDSelf;
  Self!Common.ColumnsPalette = '$FFFFFF $F0F0F0 $FFFFFF $F0F0F0';
  Self!Common.SelFillColor =  $FFBFBF;
  Self!Common.SelFontColor =  $0;
  Self!Common.DisFillColor =  $F0F0F0;
  Self!Common.DisFontColor =  $0F0F0F;
//    Hex(Bit(Ord(K_ramSkipRowsInfo))) +
//  Self!Common.ModeFlags =
//    Hex(Bit(Ord(K_ramFillFrameWidth))) +
//    Hex(Bit(Ord(K_ramUseFillColor)));
  Self!Common.ModeFlags = "[K_ramUseFillColor,K_ramFillFrameWidth,K_ramSkipInfoCol,K_ramSkipColsMark]"; // K_ramSkipInfoCol
  Self!Common.ColResizeMode = K_crmNormal;
  Self!Common.DSeparatorSize = 5;
  SetLength( Adr(Self!Columns), 100 ); // initial value
  GColumns = Self!Columns;

  GRC.CWidth=200; // all fields width (Value column width)
  GRC.CCFillColorInd=0;
  GIndex=0;
end;

//************************************************************ N_SetSimpleRC
//  Set Simple Field Editor
//
procedure N_SetSimpleRC( FName : string; Caption : string );

begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
  GRC.CHint = Caption;
  if (N_DisableField NE 0) 
    GRC.CShowEditFlags=GRC.CShowEditFlags + Hex(Bit(Ord(K_racCDisabled)));

  GColumns[GIndex] = GRC;

  GIndex = GIndex + 1;
end; // procedure N_SetSimpleRC

//************************************************************ N_SetSimpleRCH
//  Set Simple Field Editor and Hint
//
procedure N_SetSimpleRCH( FName: string; Caption: string; Hint: string );

begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
  GRC.CHint = Hint;
//  if (N_DisableField NE 0) GRC.CShowEditFlags=Hex(Bit(Ord(K_racReadOnly)));
  if (N_DisableField NE 0) GRC.CShowEditFlags=GRC.CShowEditFlags + Hex(Bit(Ord(K_racCDisabled)));

  GColumns[GIndex] = GRC;

  GRC.CShowEditFlags=0;
  GIndex = GIndex + 1;
end; // procedure N_SetSimpleRCH


//************************************************************ N_SetBoolRC
//  Set Boolean Field Editor
//
procedure N_SetBoolRC( FName : string; Caption : string );

begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
  GRC.CBitMask = 1;
  GRC.CHint = Caption;
  if (N_DisableField NE 0) 
    GRC.CShowEditFlags=GRC.CShowEditFlags + Hex(Bit(Ord(K_racCDisabled))) + Hex(Bit(Ord(K_racShowCheckBox))));
  else
    GRC.CShowEditFlags=GRC.CShowEditFlags + Hex(Bit(Ord(K_racShowCheckBox)));

//  if (N_DisableField NE 0) GRC!CShowEditFlags=[K_racCDisabled,K_racShowCheckBox];
  GColumns[GIndex] = GRC;

  GRC.CBitMask = 0;
  GRC.CShowEditFlags=0;
  GIndex = GIndex + 1;
end; // procedure N_SetBoolRC

//************************************************************ N_SetBoolRCH
//  Set Boolean Field Editor and Hint
//
procedure N_SetBoolRCH( FName: string; Caption: string; Hint: string );

begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
  GRC.CBitMask = 1;
  GRC.CHint = Hint;
  if (N_DisableField NE 0) 
    GRC.CShowEditFlags=GRC.CShowEditFlags + Hex(Bit(Ord(K_racCDisabled))) + Hex(Bit(Ord(K_racShowCheckBox))));
  else 
    GRC.CShowEditFlags=GRC.CShowEditFlags + Hex(Bit(Ord(K_racShowCheckBox)));
//  if (N_DisableField NE 0) GRC!CShowEditFlags="[K_racCDisabled,K_racShowCheckBox]";
  GColumns[GIndex] = GRC;

  GRC.CBitMask = 0;
  GRC.CShowEditFlags=0;
  GIndex = GIndex + 1;
end; // procedure N_SetBoolRCH

//************************************************************ N_SetSetRC
//  Set Set Field Editor
//
procedure N_SetSetRC( FName : string; Caption : string );

begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
//  GRC.CShowEditFlags=Hex(Bit(Ord(K_racExternalEditor)));
  GRC.CShowEditFlagSet = "[K_racNotModalShow,K_racExternalEditor]";
  GRC.CHint = Caption;
  GColumns[GIndex] = GRC;

  GRC.CShowEditFlags=0;
  GIndex = GIndex + 1;
end; // procedure N_SetSetRC

//************************************************************ N_SetSetRCH
//  Set Set Field Editor and Hint
//
procedure N_SetSetRCH( FName: string; Caption: string; Hint: string );

begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
  GRC.CShowEditFlagSet = "[K_racNotModalShow,K_racExternalEditor]";
  GRC.CHint = Hint;
  GColumns[GIndex] = GRC;

  GRC.CShowEditFlags=0;
  GIndex = GIndex + 1;
end; // procedure N_SetSetRCH

//************************************************************ N_SetArrayRC
//  Set Array with External Editor
//
procedure N_SetArrayRC( FName : string; Caption : string );

begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
  GRC.CShowEditFlags=Hex(Bit(Ord(K_racExternalEditor)));
  GRC.CHint = Caption;
  GColumns[GIndex] = GRC;

  GRC.CShowEditFlags=0;
  GIndex = GIndex + 1;
  Exit;

//  GColumns[GIndex]!CShowEditFlags=0; // compiler error!
//  if FName ne "" then Exit;
end; // procedure N_SetArrayRC

//************************************************************ N_SetArrayRCH
//  Set Array with External Editor and Hint
//
procedure N_SetArrayRCH( FName: string; Caption: string; Hint: string );

begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
  GRC.CShowEditFlags=Hex(Bit(Ord(K_racExternalEditor)));
  GRC.CHint = Hint;
  GColumns[GIndex] = GRC;

  GRC.CShowEditFlags=0;
  GIndex = GIndex + 1;
end; // procedure N_SetArrayRCH

//************************************************************ N_SetTextRC
//  Set Text (with Line Breakes) Editor
//
procedure N_SetTextRC( FName : string; Caption : string );

begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
  GRC.CEditors = "RAFSArrEditor";
  GRC.CHint = Caption;
  GColumns[GIndex] = GRC;

  GRC.CEditors = SubArray( GRC.CEditors, 0, 0, 0 );
  GIndex = GIndex + 1;
end; // procedure N_SetTextRC

//************************************************************ N_SetTextRCH
//  Set Text (with Line Breakes) Editor and Hint
//
procedure N_SetTextRCH( FName: string; Caption: string; Hint: string );

var
 TmpGRC : TK_RAFColumnDescr;
begin
  TmpGRC = GRC;
  SetLength ( Adr(TmpGRC.CVEAttrs), 3 );

  TmpGRC.FName = FName;
  TmpGRC.CCaption = Caption;
  TmpGRC.CHint = Hint;

  TmpGRC.CVEAttrs[0]!CEName = "RAFSArrEditor";
  TmpGRC.CVEAttrs[0]!CVName = "RAFSArrEditor";
  TmpGRC.CVEAttrs[1]!CEName = "NRAFCharCodesEditor";
//  TmpGRC.CVEAttrs[1]!CVName = "NRAFCharCodesEditor";
  TmpGRC.CVEAttrs[2]!CEName = "NoEditor";

  GColumns[GIndex] = TmpGRC;
  GIndex = GIndex + 1;

{
begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
  GRC.CEditors = "RAFSArrEditor";
  GRC.CHint = Hint;
  GColumns[GIndex] = GRC;

  GRC.CEditors = SubArray( GRC.CEditors, 0, 0, 0 );
  GIndex = GIndex + 1;
}
end; // procedure N_SetTextRCH

//************************************************************ N_SetFontRCH
//  Set Font Editor and Hint
//
procedure N_SetFontRCH( FName: string; Caption: string; Hint: string );

begin
  GRC.FName = FName;
  GRC.CCaption = Caption;
  GRC.CHint = Hint;

  GColumns[GIndex] = GRC;

  GIndex = GIndex + 1;
end; // procedure N_SetFontRCH

//************************************************************ N_SetExtFormRC
//  Set External Form Editor with given FormDescr on whole record
//
procedure N_SetExtFormRC( AFormDescr : string; Caption : string );

begin
  GRC.FName = ""; // special value for whole record
  GRC.CCaption = Caption;
  GRC.CGEDataID  = "N_RAEditForm";
  GRC.CEdUserPar = AFormDescr;
  GRC.CHint = Caption;
  GColumns[GIndex] = GRC;

  GRC.CGEDataID  = "";
  GRC.CEdUserPar = "";
  GIndex = GIndex + 1;
end; // procedure N_SetExtFormRC

//************************************************************ N_SetExtFormRCH
//  Set External Form Editor with given FormDescr on whole record and Hint
//
procedure N_SetExtFormRCH( AFormDescr: string; Caption: string; Hint: string );

begin
  GRC.FName = ""; // special value for whole record
  GRC.CCaption = Caption;
  GRC.CGEDataID  = "N_RAEditForm";
  GRC.CEdUserPar = AFormDescr;
  GRC.CHint = Hint;
  GColumns[GIndex] = GRC;

  GRC.CGEDataID  = "";
  GRC.CEdUserPar = "";
  GIndex = GIndex + 1;
end; // procedure N_SetExtFormRCH

//********************************************************** N_AddFNameEditors
//  Add External FileName Editors to previous RC
//  ( Add RAFFileName and RAFFNameCmBEditor )
//
procedure N_AddFNameEditors( AParams: string );
var
 PS: ^ArrayOf String;
 TmpGRC : TK_RAFColumnDescr;
begin
  TmpGRC = GColumns[GIndex-1];
  SetLength ( Adr(TmpGRC.CVEAttrs), 2 );

  TmpGRC.CVEAttrs[0]!CEName   = "RAFFileName";
  TmpGRC.CVEAttrs[0]!CVName   = "RAFFileName";
  TmpGRC.CVEAttrs[0]!CEParams = AParams;
  TmpGRC.CVEAttrs[0]!CVParams = AParams;
  TmpGRC.CVEAttrs[1]!CEName   = "RAFFNameCmBEditor";
  TmpGRC.CVEAttrs[1]!CEParams = AParams;

  GColumns[GIndex-1] = TmpGRC;
end; // procedure N_AddFNameEditors

//************************************************************** N_AddExtEditor
//  Add External Editor to previous RC
//
procedure N_AddExtEditor( AExtEditorName: string; AUserPar: string );
var
 PS: ^ArrayOf String;
 TmpGRC : TK_RAFColumnDescr;
begin
  TmpGRC = GColumns[GIndex-1];
  PS = Adr(TmpGRC.CEditors);
  SetLength( PS, 1 );

  if(  AUserPar NE "" ) TmpGRC.CEdUserPar = AUserPar;
  TmpGRC.CEditors[0] = AExtEditorName;
  GColumns[GIndex-1] = TmpGRC;
end; // procedure N_AddExtEditor

//********************************************************** N_AddTwoExtEditors
//  Add Two External Editors with FormDescriptions to previous RC
//
procedure N_AddTwoExtEditors( AEd1Name: string; AFD1Name: string; AEd2Name: string; AFD2Name: string );
var
 PS: ^ArrayOf String;
 TmpGRC : TK_RAFColumnDescr;
begin
  TmpGRC = GColumns[GIndex-1];
  SetLength ( Adr(TmpGRC.CVEAttrs), 2 );

  TmpGRC.CVEAttrs[0]!CEName   = AEd1Name;
  TmpGRC.CVEAttrs[0]!CEParams = AFD1Name;
  TmpGRC.CVEAttrs[1]!CEName   = AEd2Name;
  TmpGRC.CVEAttrs[1]!CEParams = AFD2Name;

  GColumns[GIndex-1] = TmpGRC;
end; // procedure N_AddTwoExtEditors

//************************************************************ N_Add2ExtEditors
//  Add Two External Editors to previous RC
//
procedure N_Add2ExtEditors( AExtEd1Name: string; AExtEd2Name: string; AUserPar: string );
var
 PS: ^ArrayOf String;
 TmpGRC : TK_RAFColumnDescr;
begin
  TmpGRC = GColumns[GIndex-1];
  PS = Adr(TmpGRC.CEditors);
  SetLength( PS, 2 );

  if(  AUserPar NE "" ) TmpGRC.CEdUserPar = AUserPar;
  TmpGRC.CEditors[0] = AExtEd1Name;
  TmpGRC.CEditors[1] = AExtEd2Name;
  GColumns[GIndex-1] = TmpGRC;
end; // procedure N_Add2ExtEditors

//************************************************************ N_Add3ExtEditors
//  Add Three External Editors to previous RC
//
procedure N_Add3ExtEditors ( AExtEd1Name: string; AExtEd2Name: string; AExtEd3Name: string; AUserPar: string );
var
 PS: ^ArrayOf String;
 TmpGRC : TK_RAFColumnDescr;
begin
  TmpGRC = GColumns[GIndex-1];
  PS = Adr(TmpGRC.CEditors);
  SetLength( PS, 3 );

  if(  AUserPar NE "" ) TmpGRC.CEdUserPar = AUserPar;
  TmpGRC.CEditors[0] = AExtEd1Name;
  TmpGRC.CEditors[1] = AExtEd2Name;
  TmpGRC.CEditors[2] = AExtEd3Name;
  GColumns[GIndex-1] = TmpGRC;
end; // procedure N_Add3ExtEditors

//************************************************************ N_AddExtViewer
//  Add External Viewer to previous RC
//
procedure N_AddExtViewer( AExtViewerName: string; AUserPar: string );
var
 PS: ^ArrayOf String;
 TmpGRC : TK_RAFColumnDescr;
begin
  TmpGRC = GColumns[GIndex-1];
  PS = Adr(TmpGRC.CViewers);
  SetLength( PS, 1 );

  if( AUserPar NE "" ) TmpGRC.CEdUserPar = AUserPar;
  TmpGRC.CViewers[0] = AExtViewerName;
  GColumns[GIndex-1] = TmpGRC;
end; // procedure N_AddExtViewer

//************************************************************ N_AddExtEdViewer
//  Add External Editor and Viewer to previous RC
//
procedure N_AddExtEdViewer( AExtEditorName: string; AExtViewerName: string; AUserPar: string );
var
 PS1: ^ArrayOf String;
 PS2: ^ArrayOf String;
begin
  PS1 = Adr(GRC.CEditors);
  SetLength( PS1, 1 );
  GRC.CEditors[0] = AExtEditorName;

  PS2 = Adr(GRC.CViewers);
  SetLength( PS2, 1 );
  GRC.CViewers[0] = AExtViewerName;

  GRC.CEdUserPar = AUserPar;
  GColumns[GIndex-1] = GRC;

  GRC.CViewers = SubArray( GRC.CViewers, 0, 0, 0 );
  GRC.CEditors = SubArray( GRC.CEditors, 0, 0, 0 );
  GRC.CEdUserPar = "";
end; // procedure N_AddExtEdViewer

//*************************************************************** N_AddVArrayVE
//  Add Two External Editors and one Viewer to previous RC for VArray field
//
procedure N_AddVArrayVE( AExtEditorName: string; AUserPar: string );
var
 PS1: ^ArrayOf String;
 PS2: ^ArrayOf String;
begin
  PS1 = Adr(GRC.CEditors);
  SetLength( PS1, 2 );
  GRC.CEditors[0] = AExtEditorName;
  GRC.CEditors[1] = "RAFVArrayEditor";

  PS2 = Adr(GRC.CViewers);
  SetLength( PS2, 1 );
  GRC.CViewers[0] = AExtEditorName;

  GRC.CEdUserPar = AUserPar;
  GColumns[GIndex-1] = GRC;

  GRC.CViewers = SubArray( GRC.CViewers, 0, 0, 0 );
  GRC.CEditors = SubArray( GRC.CEditors, 0, 0, 0 );
  GRC.CEdUserPar = "";
end; // procedure N_AddVArrayVE

//************************************************************ N_SetSeparatorRC
//  Set Separator
//
procedure N_SetSeparatorRC( ACaption: string );

begin
  GRC.FName="";
  GRC.CCaption = ACaption;
  GRC.CShowEditFlags=Hex(Bit(Ord(K_racSeparator)));
  GColumns[GIndex] = GRC;

  GRC.CShowEditFlags=0;
  GIndex = GIndex + 1;
end; // procedure N_SetSeparatorRC


