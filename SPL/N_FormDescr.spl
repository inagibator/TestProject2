//***************************  TN_VCTreeParamsFormDescr  *********************
type TN_VCTreeParamsFormDescr = class
  Common : TK_RAFrDescr;
  Columns:  ArrayOf TK_RAFColumnDescr;
  constructor Create( );
end; // type TN_VCTreeParamsFormDescr = class

constructor TN_VCTreeParamsFormDescr!Create( );
begin
  N_InitCommon( Result );

  N_SetSimpleRCH ( "VCTName",        "VCTree Name",       "Имя картограммы" );
  N_SetSimpleRCH ( "VCTHeaderText",  "VCTree HeaderText", "Заголовок картограммы" );
  N_SetSimpleRCH ( "VCTPageNumber",  "VCTree PageNumber", "Номер страницы для печати" );
  N_SetArrayRCH  ( "VCTLayers",      "VCTree Layers",     "Слои картограммы" );
  N_AddExtEditor ( "NRAFRaEditEditor", "" );

  SetLength( Adr(Self!Columns), GIndex );
end; // constructor TN_VCTreeParamsFormDescr!Create( );

